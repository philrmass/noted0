{
  "name": "noted",
  "version": "1.0.0",
  "description": "Notes app",
  "main": "index.js",
  "homepage": "https://philrmass.github.io/noted",
  "babel": {
    "plugins": [
      "@babel/plugin-transform-runtime"
    ],
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "scripts": {
    "build": "webpack --mode production",
    "deploy": "gh-pages -d dist",
    "lint": "eslint src",
    "predeploy": "npm run build",
    "start": "webpack serve --config ./webpack.config.js --mode development",
    "test": "jest"
  },
  "author": "Phil Mass",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "eslint": "^7.29.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.24.0",
    "gh-pages": "^3.2.3",
    "style-loader": "^3.0.0",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "uuid": "^3.4.0"
  }
}
